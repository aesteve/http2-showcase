<!DOCTYPE html>
<html>
    <head>
        <script>
            var start = new Date().getTime();
            window.onload = function() {
                var end = new Date().getTime();
                var total = end - start;
                document.querySelector('#time').innerHTML = 'Loaded in: ' + total + ' ms';
            };
        </script>
    </head>
    <body style="line-height: 1">
        <p>
        {{#each imgs}}
            {{#each this}}
                <img width="{{tileWidth}}" height="{{tileHeight}}" src="{{this}}" style="border:none;margin:0" />
            {{/each}}
            <br/>
        {{/each}}
        </p>
        <p id="time">Loading...</p>
    </body>
</html>